{
    "name": "oguz463/php-gedcom",
    "description": "A GEDCOM file parser (read + write) for PHP 5.3+",
    "type": "library",
    "keywords": ["gedcom","parser"],
    "homepage": "http://github.com/oguz463/php-gedcom",
    "license": "GPL-3.0",
    "require": {
        "php": ">=5.3",
        "monolog/monolog": "dev-bugfix"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-0": {
            "PhpGedcom\\": "library/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/skypal/php-gedcom"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mrkrstphr/php-gedcom"
        }
    ]
}
